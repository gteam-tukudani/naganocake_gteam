<h1>配送先登録 / 一覧</h1>
<%= form_with model: @address, url: address_path(@address), local:true do |f| %>
  <p>郵便番号(ハイフンなし)</p><p><%= f.text_field :postal_code %></p>
  <p>住所</p><p><%= f.text_field :address %></p>
  <p>宛名</p><p><%= f.text_field :name %></p>
  <%= f.submit "変更を保存する", class: "btn btn-success" %>
<% end %>